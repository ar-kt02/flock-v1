{
  "name": "project-v1-launchpad",
  "version": "1.0.0",
  "description": "A backend service for an events platform.",
  "main": "dist/index.js",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev src/index.ts",
    "test": "NODE_ENV=test jest --runInBand",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development prisma migrate dev",
    "prisma:migrate:test": "dotenv -e .env.test prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio:dev": "dotenv -e .env.development prisma studio",
    "seed": "dotenv -e .env.development ts-node prisma/seed.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.3",
    "@fastify/oauth2": "^8.1.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@prisma/client": "^6.3.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.8",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "ts-loader": "^9.5.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
